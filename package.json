{
  "name": "savedfeast-web",
  "version": "1.0.0",
  "description": "SavedFeast Web Application - A modern food delivery platform built with Laravel and React",
  "keywords": [
    "laravel",
    "react",
    "typescript",
    "food-delivery",
    "sustainability",
    "restaurant-platform",
    "api"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://github.com/yourusername"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/savedfeast-web.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/savedfeast-web/issues"
  },
  "homepage": "https://github.com/yourusername/savedfeast-web#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "serve:full": "concurrently \"php artisan serve\" \"npm run dev\"",
    "serve:backend": "php artisan serve",
    "serve:frontend": "npm run dev",
    "setup": "npm install && composer install && npm run build",
    "clean": "rm -rf node_modules vendor dist",
    "format": "prettier --write \"resources/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"resources/**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.0",
    "chart.js": "^4.4.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@eslint/js": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.34.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.57.0",
    "globals": "^13.24.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "sass": "^1.69.0",
    "typescript": "^5.3.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0"
  },
  "engines": {
    "node": ">=18.8.0",
    "npm": ">=8.0.0"
  }
}
